    {
    "manifest_version": 3,
    "name": "Googol Translator",
    "description": "Translate websites on the fly with Googol Translate",
    "version": "3.9",
    "web_accessible_resources": [
        {
            "resources": ["./mesagges.json"],
            "matches": ["<all_urls>"]
        }
    ],

    "action": {
        "default_icon": "icon.png"
    },


    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],

            "js": [
                "index.js"
            ]
        }
    ]

    }
